{
  "name": "@mapbox/leaflet-pip",
  "version": "1.1.0",
  "description": "point in polygon searches for leaflet",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && npm run test-unit",
    "test-unit": "browserify test/*.js | tape-run",
    "lint": "eslint index.js",
    "build-site": "browserify site/site.js > site/bundle.js",
    "prepublish": "browserify -s leafletPip index.js > dist/leaflet-pip.js && uglifyjs dist/leaflet-pip.js -c -m > dist/leaflet-pip.min.js"
  },
  "files": [
    "index.js",
    "dist/leaflet-pip.js",
    "dist/leaflet-pip.min.js"
  ],
  "keywords": [
    "leaflet",
    "plugin",
    "point",
    "in",
    "polygon",
    "geo",
    "geographical"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/mapbox/leaflet-pip.git"
  },
  "devDependencies": {
    "browserify": "^14.4.0",
    "eslint": "^4.8.0",
    "phantomjs": "^2.1.7",
    "tape": "^4.8.0",
    "tape-run": "^3.0.0",
    "uglify-js": "^3.1.3"
  },
  "author": "Tom MacWright <tom@macwright.org>",
  "license": "BSD-2-Clause",
  "dependencies": {
    "geojson-utils": "~1.1.0"
  }
}
